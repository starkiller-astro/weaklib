wlComparison = \
  $(WL_LIBRARY) \
  $(WL_EOSSOURCE) \
  $(SHAREDMODULES) \
  $(COMMON_MODS_OBJ)

all: wlCalculateSoundSpeed

wlCalculateSoundSpeed: \
	$(wlComparison) wlCalculateSoundSpeed.o
	$(FLINKER) $(FLAGS) -o wlCalculateSoundSpeed_$(MACHINE) \
	$(wlComparison) wlCalculateSoundSpeed.o \
	$(LIBRARIES)

clean:
	rm -f *.o *.mod *.ld

clobber: clean
	rm wlCalculateSoundSpeed_$(MACHINE)

wlCalculateSoundSpeed.o: \
  wlKindModule.o \
